--synthesis_resources = reg 5 
OPTIONS ALTERA_INTERNAL_OPTION = "AUTO_SHIFT_REGISTER_RECOGNITION=OFF";

SUBDESIGN dffpipe_tu8
( 
	clock	:	input;
	d[4..0]	:	input;
	q[4..0]	:	output;
) 
VARIABLE 
	dffe9a[4..0] : dffe;
	clrn	: NODE;
	ena	: NODE;
	prn	: NODE;
	sclr	: NODE;

BEGIN 
	dffe9a[].clk = clock;
	dffe9a[].clrn = clrn;
	dffe9a[].d = (d[] & (! sclr));
	dffe9a[].ena = ena;
	dffe9a[].prn = prn;
	clrn = VCC;
	ena = VCC;
	prn = VCC;
	q[] = dffe9a[].q;
	sclr = GND;
END;
