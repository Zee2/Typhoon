--synthesis_resources = reg 8 
OPTIONS ALTERA_INTERNAL_OPTION = "{-to counter1a0} POWER_UP_LEVEL=HIGH;{-to parity2} POWER_UP_LEVEL=HIGH";

SUBDESIGN a_graycounter_nn6
( 
	clock	:	input;
	cnt_en	:	input;
	q[4..0]	:	output;
) 
VARIABLE 
	counter1a0 : dffeas
		WITH (
			power_up = "high"
		);
	counter1a1 : dffeas;
	counter1a2 : dffeas;
	counter1a3 : dffeas;
	counter1a4 : dffeas;
	parity2 : dffeas
		WITH (
			power_up = "high"
		);
	sub_parity3a[1..0] : dffeas;
	cntr_cout[4..0]	: WIRE;
	parity_cout	: WIRE;
	sclr	: NODE;
	updown	: NODE;

BEGIN 
	counter1a[4..0].clk = clock;
	counter1a[4..0].d = ( (counter1a[4].q $ cntr_cout[3..3]), (counter1a[3].q $ (counter1a[2].q & cntr_cout[2..2])), (counter1a[2].q $ (counter1a[1].q & cntr_cout[1..1])), (counter1a[1].q $ (counter1a[0].q & cntr_cout[0..0])), ((cnt_en & (counter1a[0].q $ (! parity_cout))) # ((! cnt_en) & counter1a[0].q)));
	counter1a[4..0].sclr = sclr;
	parity2.clk = clock;
	parity2.d = ((cnt_en & (sub_parity3a[0..0].q $ sub_parity3a[1..1].q)) # ((! cnt_en) & parity2.q));
	parity2.sclr = sclr;
	sub_parity3a[].clk = ( clock, clock);
	sub_parity3a[].d = ( ((cnt_en & counter1a[4..4].q) # ((! cnt_en) & sub_parity3a[1].q)), ((cnt_en & (((counter1a[0..0].q $ counter1a[1..1].q) $ counter1a[2..2].q) $ counter1a[3..3].q)) # ((! cnt_en) & sub_parity3a[0].q)));
	sub_parity3a[].sclr = ( sclr, sclr);
	cntr_cout[] = ( B"0", (cntr_cout[2..2] & (! counter1a[2].q)), (cntr_cout[1..1] & (! counter1a[1].q)), (cntr_cout[0..0] & (! counter1a[0].q)), (cnt_en & parity_cout));
	parity_cout = (((! parity2.q) $ updown) & cnt_en);
	q[] = counter1a[4..0].q;
	sclr = GND;
	updown = VCC;
END;
