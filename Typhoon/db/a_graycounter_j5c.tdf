--synthesis_resources = reg 8 
OPTIONS ALTERA_INTERNAL_OPTION = "suppress_da_rule_internal=S102;{-to counter4a0} POWER_UP_LEVEL=HIGH;{-to parity5} POWER_UP_LEVEL=HIGH";

SUBDESIGN a_graycounter_j5c
( 
	clock	:	input;
	cnt_en	:	input;
	q[4..0]	:	output;
) 
VARIABLE 
	counter4a0 : dffeas
		WITH (
			power_up = "high"
		);
	counter4a1 : dffeas;
	counter4a2 : dffeas;
	counter4a3 : dffeas;
	counter4a4 : dffeas;
	parity5 : dffeas
		WITH (
			power_up = "high"
		);
	sub_parity6a[1..0] : dffeas;
	cntr_cout[4..0]	: WIRE;
	parity_cout	: WIRE;
	sclr	: NODE;
	updown	: NODE;

BEGIN 
	counter4a[4..0].clk = clock;
	counter4a[4..0].d = ( (counter4a[4].q $ cntr_cout[3..3]), (counter4a[3].q $ (counter4a[2].q & cntr_cout[2..2])), (counter4a[2].q $ (counter4a[1].q & cntr_cout[1..1])), (counter4a[1].q $ (counter4a[0].q & cntr_cout[0..0])), ((cnt_en & (counter4a[0].q $ (! parity_cout))) # ((! cnt_en) & counter4a[0].q)));
	counter4a[4..0].sclr = sclr;
	parity5.clk = clock;
	parity5.d = ((cnt_en & (sub_parity6a[0..0].q $ sub_parity6a[1..1].q)) # ((! cnt_en) & parity5.q));
	parity5.sclr = sclr;
	sub_parity6a[].clk = ( clock, clock);
	sub_parity6a[].d = ( ((cnt_en & counter4a[4..4].q) # ((! cnt_en) & sub_parity6a[1].q)), ((cnt_en & (((counter4a[0..0].q $ counter4a[1..1].q) $ counter4a[2..2].q) $ counter4a[3..3].q)) # ((! cnt_en) & sub_parity6a[0].q)));
	sub_parity6a[].sclr = ( sclr, sclr);
	cntr_cout[] = ( B"0", (cntr_cout[2..2] & (! counter4a[2].q)), (cntr_cout[1..1] & (! counter4a[1].q)), (cntr_cout[0..0] & (! counter4a[0].q)), (cnt_en & parity_cout));
	parity_cout = (((! parity5.q) $ updown) & cnt_en);
	q[] = counter4a[4..0].q;
	sclr = GND;
	updown = VCC;
END;
